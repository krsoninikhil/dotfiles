apiVersion: extensions/v1beta1
kind: Ingress
metadata:
  name: payoutapi
  namespace: payoutapi
  annotations:
    kubernetes.io/ingress.class: nginx
    nginx.ingress.kubernetes.io/rewrite-target: /$1
spec:
  rules:
  - host: test-k8s.cashfree.com
    http:
      paths:
      - path: /payoutapi/?(.*)
        backend:
          serviceName: payoutapi
          servicePort: 80