package delivery

import (
	"cashgramuiapi/logger"
	"pgapi/models"

	"github.com/labstack/echo"
)

// NotFoundHandler handles calls for invalid request URLs
func NotFoundHandler(c echo.Context) error {
	logger.Log.Error("NotFoundHandler::request received in handler")
	return getErrorJSON(c, models.BadRequest{BaseError: models.BaseError{ErrType: models.ErrBadURL}})
}
