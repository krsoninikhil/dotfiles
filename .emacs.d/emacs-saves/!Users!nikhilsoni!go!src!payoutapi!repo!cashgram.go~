package repo

import (
	"context"
	"payoutapi/domains"
)

// Cashgram defines interface for cashgram related operations
type Cashgram interface {
	Create(context.Context, *domains.CreateCashgramRequest) (*domains.PayoutResponse, error)
	GetStatus(context.Context, string) (*domains.PayoutResponse, error)
	Deactivate(context.Context, string) (*domains.PayoutResponse, error)
}
