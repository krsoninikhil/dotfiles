apiVersion: extensions/v1beta1
kind: Ingress
metadata:
  name: cashfree-library-gamma
  namespace: cashfree-library-gamma
  annotations:
    kubernetes.io/ingress.class: nginx
    nginx.ingress.kubernetes.io/rewrite-target: /$1
spec:
  rules:
  - host: gamma.cashfree.com
    http:
      paths:
      - path: /notificationservice/?(.*)
        backend:
          serviceName: cashfree-library-gamma
          servicePort: 80
