#[no_std]
#[no_main]

use core::panic::PanicInfo;
use phil_opp_rust_os::{exit_qemu, serial_println};

fn panic(_info: &PanicInfo) -> ! {
    serial_println!("ok");  // to detect as success as this testing panic

    unsafe { exit_qemu(); }
    loop {}
}

#[no_mangle]
pub extern "C" fn _start() -> ! {
    panic!();
}
