package service

import "context"

// Cashgram defines interface for service
type Cashgram interface {
	Create(ctx context.Context, p models.CreateRequest) (models.Response, error)
	ShowStatus(ctx context.Context, p models.ShowStatusRequest) (models.Response, error)
	Destroy(ctx context.Context, p models.DestroyRequest) (models.Response, error)
}
