import os
from flask import Flask
from flask_restful import Resource, Api


app = Flask(__name__)
api = Api(app)


class PayoutRisk:

    def get(self):
        pass


api.add_resource(PayoutRisk, '/')

if __name__ == '__main__':
    STAGE = os.environ['STAGE']
    debug = False if STAGE == 'PROD' else True
    app.run(debug=debug)
