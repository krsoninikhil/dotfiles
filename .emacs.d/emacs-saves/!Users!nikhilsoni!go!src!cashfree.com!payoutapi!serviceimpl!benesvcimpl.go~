package serviceimpl

import (
	"context"

	"cashfree.com/payout/domains"
)

type Bene struct{}

func (b *Bene) Create(c context.Context, params domains.AddBeneRequest) (domains.AddBeneResponse, error) {
	return domains.AddBeneResponse{Message: "New bene created!"}, nil
}

func (b *Bene) CreateGroup(c context.Context, params domains.CreateGroupRequest) (domains.CreateGroupResponse, error) {
	return domains.CreateGroupResponse{Message: "New bene group created!"}, nil
}

