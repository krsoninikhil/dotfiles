package service

type BeneSvc interface {
	Create(c echo.Context, params delivery.AddBeneRequest) delivery.AddBeneResponse
	CreateGroup(c echo.Context, params delivery.CreateGroupRequest) delivery.CreateGroupResponse
	ShowAll(c echo.Context, params delivery.GetBenesRequest) delivery.GetBenesResponse
	Show(c echo.Context, params delivery.GetBeneInfoRequest) delivery.GetBeneInfoResponse
	GetBeneId(c echo.Context, params delivery.GetBeneIdRequest) delivery.GetBeneIdResponse
	Destroy(c echo.Context, params delivery.RemoveBeneRequest) delivery.RemoveBeneRequest
}
